.PHONY: lib test

all: lib test

lib:
	g++ -std=c++11 -o libtest.so -rdynamic -DMXNET_CUSTOM_OP -shared -fpic test.cc -I../../include/mxnet  -I../../3rdparty/dlpack/include

test:
	g++ -std=c++11 -o test test_customOp.cc -ldl -I../../include/mxnet

clean:
	rm -rf test
ultraclean: clean
	rm -rf *.so
